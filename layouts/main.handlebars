<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <title>{{htmlTitle}}</title>
  <link rel="stylesheet" href="{{projectAssets}}/vendor/bootstrap/css/bootstrap.css">
  <link rel="stylesheet" href="{{projectAssets}}/vendor/bootstrap/css/bootstrap-theme.css">
  <link rel="stylesheet" href="{{projectAssets}}/vendor/prettify/prettify.css">
  <link rel="stylesheet" href="{{projectAssets}}/css/yuidoc-bootstrap.css">
  <link rel="shortcut icon" type="image/png" href="{{projectAssets}}/favicon.png?v=1">
</head>
<body>
<nav class="navbar navbar-default navbar-static-top" role="navigation">
  <div class="navbar-header">
    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse">
      <span class="sr-only">Toggle navigation</span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </button>
    <a class="navbar-brand" href="{{projectRoot}}index.html">{{projectName}}</a>
  </div>
  <div class="collapse navbar-collapse" id="navbar-collapse">
    <p class="navbar-text navbar-right">v{{projectVersion}}</p>
  </div>
</nav>

<div class="container" id="main">
  <div class="row">
    <div class="col-md-3">
    {{>sidebar}}
    </div>
    <div class="col-md-9">
      {{>options}}
      <div class="apidocs">
        <div id="docs-main">
          <div class="content">
            {{>layout_content}}
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<script src="{{projectAssets}}/vendor/jquery/jquery.js"></script>
<script src="{{projectAssets}}/vendor/bootstrap/js/bootstrap.js"></script>
<script src="{{projectAssets}}/vendor/prettify/prettify.js"></script>
<script src="{{projectAssets}}/js/yuidoc-bootstrap.js"></script>
</body>
</html>
